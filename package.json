{
  "name": "y",
  "displayName": "YAML for JavaScript",
  "description": "Embedded YAML for JavaScript",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.yaml"
        ],
        "scopeName": "inline.yaml",
        "path": "./grammars/inline-yaml.json",
        "embeddedLanguages": {
          "meta.embedded.block.yaml": "yaml",
          "source.yaml": "yaml"
        },
        "tokenTypes": {
          "meta.embedded.block.yaml": "other"
        }
      },
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.yaml"
        ],
        "scopeName": "inline.yaml.js",
        "path": "./grammars/inline-yaml-js.json",
        "embeddedLanguages": {
          "meta.template.expression.ts": "typescript"
        }
      }
    ]
  }
}
