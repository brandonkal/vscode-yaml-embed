{
  "name": "y",
  "displayName": "y",
  "description": "Embedded YAML for JavaScript",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "abc",
        "extensions": [
          ".abc"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "yaml-embed",
        "extensions": [
          ".yaml-embed"
        ]
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/abc.tmLanguage.json",
        "scopeName": "source.abc",
        "language": "abc"
      },
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.js.jsx",
          "source.tsx",
          "source.vue"
        ],
        "scopeName": "abc",
        "path": "./syntaxes/yaml-embed.json",
        "embeddedLanguages": {
          "source.abc": "abc",
          "meta.embedded.line.ts": "typescript"
        }
    }
    ]
  }
}
