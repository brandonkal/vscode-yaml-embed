{
  "name": "y",
  "displayName": "y",
  "description": "Embedded YAML for JavaScript",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yaml-template"
      }
    ],
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.jsx",
          "source.js.jsx",
          "source.tsx",
          "source.vue"
        ],
        "scopeName": "inline.yaml",
        "path": "./syntaxes/yaml-embed.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.yaml": "yaml"
        }
      },
      {
        "injectTo": ["source.yaml"],
        "scopeName": "inline.ts",
        "language": "yaml",
        "path": "./syntaxes/yaml-js.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.line.ts": "typescript"
        }
      }
    ]
  }
}
